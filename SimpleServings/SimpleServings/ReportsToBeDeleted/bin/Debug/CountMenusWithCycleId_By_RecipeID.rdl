    <DataSet Name="DSrecipeID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 
                a.RecipeID, RTRIM(CAST(a.RecipeID AS CHAR(6))) + '--' + a.RecipeName AS 'Name', RTRIM(CAST(a.RecipeName AS varchar(200))) AS 'RecipeName', 1 AS display_order
FROM      tblRecipe AS a INNER JOIN
                tblMenuItem AS b ON a.RecipeID = b.RecipeID INNER JOIN
                tblMenu AS c ON b.MenuID = c.MenuID INNER JOIN
                tblContract AS f ON a.ContributorID = f.ContractID
WHERE   (b.IsAlternate = 0) AND (b.IsActive = 1) AND (a.IsActive = 1)
UNION
SELECT   '00' AS Expr1, 'ALL SELECT' AS Expr2, '' AS Expr3, 0 AS display_order
ORDER BY display_order, RTRIM(CAST(a.RecipeName AS varchar(200)))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RecipeID">
          <DataField>RecipeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RecipeName">
          <DataField>RecipeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="display_order">
          <DataField>display_order</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
